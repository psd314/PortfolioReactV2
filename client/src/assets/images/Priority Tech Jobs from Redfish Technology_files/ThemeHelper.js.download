function alertcontents(sender)
{
	var i;
	var str="";
	for (i in sender)
	{
	    str=str+i+" : ";
	    if(i%5==0)
	    	str+"\n";
	}
	alert(str);
}
function alertstylesheets()
{
    try
    {
        var i,str;
        for(i=0;i<document.styleSheets.length;i++)
        {
            alert(document.styleSheets[i].href); 
            if(document.styleSheets[i].href != null)
                str += (i+" : "+document.styleSheets[i].href+"\n");
        }
        alert(str);
    }
    catch(e)
    {
        alert(e);
    }
}